{
  "name": "tw-material3-plugin",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyam234/tw-material3.git"
  },
  "homepage": "https://github.com/divyam234/tw-material3#readme",
  "scripts": {
    "build": "tsup",
    "format": "prettier --write src",
    "prepublishOnly": "pnpm run build",
    "publish": "pnpm publish --access public"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material/material-color-utilities": "^0.2.7"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "tw-colors": "^3.3.1"
  }
}
