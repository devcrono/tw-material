{
  "name": "@tw-material/react",
  "version": "1.1.21",
  "license": "MIT",
  "main": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/divyam234/tw-material3.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/divyam234/tw-material3/issues"
  },
  "scripts": {
