    "@tw-material/popover": "workspace:*",
    "@tw-material/dropdown": "workspace:*",
    "@tw-material/modal": "workspace:*",
    "@tw-material/menu": "workspace:*",
    "@tw-material/ripple": "workspace:*",
    "@tw-material/breadcrumbs": "workspace:*",
    "@tw-material/box": "workspace:*",
    "@tw-material/avatar": "workspace:*",
    "@tw-material/tooltip": "workspace:*",
    "@tw-material/slider": "workspace:*",
    "@tw-material/input": "workspace:*",
    "@tw-material/listbox": "workspace:*",
    "@tw-material/divider": "workspace:*",
    "@tw-material/spinner": "workspace:*",
    "@tw-material/select": "workspace:*",
    "@tw-material/autocomplete": "workspace:*",
    "@tw-material/chip": "workspace:*",
